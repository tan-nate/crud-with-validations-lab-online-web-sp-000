<h2>Songs</h2>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Release year</th>
      <th>Released</th>
      <th>Genre</th>
      <th>Artist name</th>
    </tr>
  </thead>
  <tbody>
      <% @songs.each do |song| %>
        <tr>
          <td><%= song.title %></td>
          <td><%= song.release_year %></td>
          <td><%= song.released %></td>
          <td><%= song.genre %></td>
          <td><%= song.artist_name %></td>
        </tr>
      <% end %>
    </tr>
  </tbody>
</table>
